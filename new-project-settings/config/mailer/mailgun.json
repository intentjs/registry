{
  "type": "inject-config",
  "dependencies": {
    "mailgun.js": "",
    "form-data": ""
  },
  "namespace": "mailers",
  "key": ["channels", "mailgun"],
  "filename": "config/mailer.ts",
  "value": {
    "provider": "mailgun",
    "username": "env://MAILGUN_USERNAME",
    "key": "env://MAILGUN_API_KEY",
    "domain": "env://MAILGUN_DOMAIN",
    "from": "env://FROM_ADDRESS"
  },
  "env": [["DEFAULT_MAILER", "mailgun"]]
}
