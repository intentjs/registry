{
  "type": "inject-config",
  "dependencies": {
    "ioredis": "*"
  },
  "namespace": "cache",
  "key": ["stores", "redis"],
  "filename": "config/cache.ts",
  "value": {
    "driver": "redis",
    "host": "env://REDIS_HOST",
    "password": "env://REDIS_PASSWORD",
    "port": "env://REDIS_PORT",
    "database": "env://REDIS_DB",
    "prefix": "intentjs_cache"
  }
}
