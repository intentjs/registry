{
  "type": "inject-config",
  "dependencies": {
    "better-sqlite3": "*"
  },
  "namespace": "db",
  "key": ["connections", "sqlite"],
  "filename": "config/db.ts",
  "value": {
    "client": "better-sqlite3",
    "debug": "!!+env://DB_DEBUG",
    "connection": {
      "filename": "./inbtent.db"
    },
    "useNullAsDefault": true,
    "migrations": {
      "directory": "./database/migrations"
    }
  }
}
